int i, j;

read(i);

switch (i) {
    case 1: write(1);
    case 2: write(2);
    case 3: write(3);
    case 4: write(4);
    case 5: {
        write(5);
        break;
    }
    case 6: {
        read(j);
        switch (j) {
            case 1: break;
            case 2: {
                write(2);
                break;
            }
            case 5: write(6);
        }
    }
    default: write(0);
}