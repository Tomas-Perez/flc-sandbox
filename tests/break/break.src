int i, j;

i = 0;

while(i<10) {
    write(i);
    i = i + 1;
    if(i > 5) {
        break;
    }
}

write(9999); /* Separator between tests */

i = 0;

while(i < 3) {
    write(1111); /* Separator between loops */
    j = 0;
    while(j < 10) {
        if (j > i) {
            break;
        }
        write(j);
        j = j + 1;
    }
    i = i + 1;
}