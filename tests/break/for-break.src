int i, j;

for(i=0;i<10;i = i + 1){
    if(i > 5) {
        break;
    }
    write(i);
}

write(9999); /* Separator between tests */

for(i = 0; i<3; i = i + 1) {
    write(1111); /* Separator between loops */
    for(j = 0; j < 10; j = j + 1) {
        if (j > i) {
            break;
        }
        write(j);
    }
}

write(9999);
for(i=0;1; i = i + 1) {
    if(i >= 10) {
        break;
    }
    write(i);
}